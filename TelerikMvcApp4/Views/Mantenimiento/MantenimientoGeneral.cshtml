                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <div class="col-lg-10">
                                                    <p><strong>Clase:</strong></p>
                                                    @(Html.Kendo().ComboBox()
                                                  .Name("clase2")
                                                  .DataTextField("DescClase")
                                                  .DataValueField("CodClase")
                                                  .Filter(FilterType.StartsWith)
                                                  .IgnoreCase(true)
                                                  .Placeholder("Selecciona una clase...")
                                                  .DataSource(source =>
                                                  {
                                                      source.Read(read =>
                                                      {
                                                          read.Action("GetClase", "Gestion");
                                                      });
                                                  })
                                                    )
