@*@model IEnumerable<TelerikMvcApp4.Models.MntPoder>*@

@{
    ViewBag.Title = "Mantenimientos";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    @*<div class="container body-content">*@
        <div class="alert alert-dismissible alert-success successDiv" id="kokotecheId" style="display:none;">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <strong>Well done!</strong> You successfully read <a href="#" class="alert-link">this important alert message</a>.
        </div>


        <section class="container-fluid">
            <div class="row">
                <div class="col-lg-6" style="width:15%;height:100%;position:fixed;text-align:left;font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;">
                    <div class="list-group">
                        <a class="list-group-item" href="#sistema">Sistema</a>
                        <a class="list-group-item" href="#empresa">Titular</a>
                        <a class="list-group-item" href="#Poder">Poder</a>
                        <a class="list-group-item" href="#Agente">Agente</a>
                        <a class="list-group-item" href="#Periodico">Periódico</a>
                        <a class="list-group-item" href="#marca">Marca</a>
                        <a class="list-group-item" href="#TipoMarca">Tipo de Marca</a>
                        <a class="list-group-item" href="#Clase">Clase</a>
                        <a class="list-group-item" href="#EdNiza">Ed Niza</a>
                        <a class="list-group-item" href="#Estado">Estado</a>
                        <a class="list-group-item" href="#Pais">País</a>
                        <a class="list-group-item" href="#TipoMov">Tipo de Movimiento</a>
                        <a class="list-group-item" href="#TipoSolicitud">Tipo de Solicitud</a>
                    </div>
                </div>

                <div class="col-lg-6" style="width:85%;height:100%;float:right;">
                    <div class="panel panel-primary panel-Mantenimiento dvSombreadoMnt" id="sistema">
                        <div class="panel-heading panel-MantenimientoHead"><p style="color:white;font-size:14px;font-family:'Segoe UI Light';">Mantenimiento del Sistema</p></div>
                        <div class="panel-body">
                            @using (Html.BeginForm("GetConfiguracion", "Mantenimiento", FormMethod.Post, new { @id = "Frm-Configuracion", role = "form", @class = "form-horizontal" }))
                            {
                                <fieldset>
                                    <legend>Mantenimiento del Sistema</legend>
                                    <div class="col-lg-6">
                                        <p> <strong>Porcentaje de parecido fonético:</strong></p>
                                        <input class="form-control" id="porcentajeParecido" name="porcentajeParecido" type="number" placeholder="Porcentaje" required autocomplete="off" onkeypress="return pulsar(event)">
                                    </div>
                                    <div class="col-lg-4">
                                        <p> <strong>Días de espera:</strong></p>
                                        <input class="form-control" id="diasEspera" name="diasEspera" type="number" placeholder="Días de espera" required autocomplete="off">
                                    </div>

                                    <div>
                                        <div class="col-lg-10" style="margin-top:20px;">
                                            <button class="btn btn-primary" type="submit">Aceptar</button>
                                            <button class="btn btn-primary" type="reset">Cancelar</button>
                                        </div>
                                    </div>
                                </fieldset>
                            }
                        </div>
                    </div>
                    <br />
                    <div class="panel panel-primary panel-Mantenimiento dvSombreadoMnt" id="empresa">
                        <div class="panel-heading panel-MantenimientoHead"><p style="color:white;font-size:14px;font-family:'Segoe UI Light';">Titular</p></div>
                        <div class="panel-body">
                            @using (Html.BeginForm("GetRegistroApoderado", "Mantenimiento", FormMethod.Post, new { @id = "Frm-loginEmpresa", role = "form", @class = "form-horizontal" }))
                            {
                                <fieldset>
                                    <legend>Mantenimiento de Titular</legend>
                                    <div class="col-lg-2">
                                        <p> <strong>Código:</strong></p>
                                        <input class="form-control" id="codigoApoderado" name="codigoApoderado" type="number" placeholder="Código" required autocomplete="off" onkeypress="return pulsar(event)">
                                    </div>
                                    <div class="form-group-sm">
                                        <div class="col-lg-6" style="margin-bottom:10px;">
                                            <p> <strong>Elige el tipo de titular:</strong></p>
                                            @(Html.Kendo().ComboBox()
                                                  .Name("comboPersona")
                                                  .DataTextField("DescTipoPersona")
                                                  .DataValueField("CodTipoPersona")
                                                  //.Filter("contains")
                                                  .Filter(FilterType.StartsWith)
                                                  .IgnoreCase(true)
                                                  .Placeholder("Seleccione un tipo de titular...")
                                                  .DataSource(source =>
                                                  {
                                                      source.Read(read =>
                                                      {
                                                          read.Action("GetTipoPersona", "Gestion");
                                                      });
                                                  })
                                            )
                                        </div>
                                    </div>
                                    <div class="form-group-sm">
                                        <div class="col-lg-10" style="margin-bottom:10px;">
                                            <p> <strong>Titular:</strong></p>
                                            <input class="form-control" id="nombreEmpresa" name="nombreEmpresa" type="text" placeholder="Razón Social" required autocomplete="off" style="text-transform: uppercase">
                                        </div>
                                    </div>
                                    <div class="form-group-sm">
                                        <div class="col-lg-10" style="margin-bottom:10px;">
                                            <p><strong>Ruc:</strong></p>
                                            <input class="form-control" id="rucEmpresa" name="rucEmpresa" type="text" placeholder="Ruc de la Empresa/Titular" required autocomplete="off">
                                        </div>
                                    </div>

                                    <div class="form-group-sm">
                                        <div class="col-lg-10" style="margin-bottom:10px;">
                                            <p><strong>Dirección:</strong></p>
                                            <input class="form-control" id="direccionEmpresa" name="direccionEmpresa" type="text" placeholder="Dirección de la Empresa/Titular" required autocomplete="off">
                                        </div>
                                    </div>

                                    <div class="form-group-sm">
                                        <div class="col-lg-6">
                                            <p><strong>Telefono:</strong></p>
                                            <input class="form-control" id="telefonoEmpresa" name="telefonoEmpresa" type="text" placeholder="Telefono Empresa/Titular" required autocomplete="off">
                                        </div>
                                    </div>
                                    <div class="form-group-sm">
                                        <div class="col-lg-6" style="margin-bottom:10px;">
                                            <p><strong>Pais:</strong></p>
                                            @(Html.Kendo().ComboBox()
                                                  .Name("pais2")
                                                  .DataTextField("PaisDescripcion")
                                                  .DataValueField("PaisCodigo")
                                                  .Filter(FilterType.StartsWith)
                                                  .IgnoreCase(true)
                                                  .Placeholder("Seleccione un país...")
                                                  .DataSource(source =>
                                                  {
                                                      source.Read(read =>
                                                      {
                                                          read.Action("GetPais", "Gestion");
                                                      });
                                                  })
                                            )

                                        </div>
                                    </div>
                                    <div class="form-group-sm">
                                        <div class="col-lg-10">
                                            <p><strong>Correo Electrónico:</strong></p>
                                            <input class="form-control" id="correoEmpresa" name="correoEmpresa" type="email" placeholder="Correo Electrónico" required autocomplete="off">
                                        </div>
                                    </div>

                                    @*<div class="form-group-sm">
                                        <p><button class="btn btn-success" type="submit">Guardar</button></p>
                                    </div>*@
                                    <div>
                                        <div class="col-lg-10" style="margin-top:20px;">
                                            <button class="btn btn-primary" type="submit">Aceptar</button>
                                            <button class="btn btn-primary" type="reset">Cancelar</button>
                                            @*<button id="editarMarcaJS" class="btn btn-primary" type="button">Editar</button>*@
                                        </div>

                                    </div>


                                </fieldset>
                            }

                        </div>
                    </div>
                    <br />
                    <div class="panel panel-primary panel-Mantenimiento dvSombreadoMnt" id="Poder">
                        <div class="panel-heading panel-MantenimientoHead"><p style="color:white;font-size:14px;font-family:'Segoe UI Light';">Poder</p></div>
                        <div class="panel-body">
                            @using (Html.BeginForm("GetRegistroPoder", "Mantenimiento", FormMethod.Post, new { @id = "Frm-loginEmpresa", role = "form", @class = "form-horizontal" }))
                            {
                                <div class="row">
                                    <div class="form-group-sm">
                                        <div class="col-lg-6">
                                            <p> <strong>Nº Poder:</strong></p>
                                            <input class="form-control" id="poderEmpresa" name="poderEmpresa" type="number" placeholder="Nº Poder" required autocomplete="off" onkeypress="return pulsarPoder(event)">
                                        </div>
                                    </div>
                                    <div class="form-group-sm">
                                        <div class="col-lg-6">
                                            <p> <strong>Titular/Solicitante:</strong></p>
                                            @(Html.Kendo().ComboBox()
                                              .Name("EmprLst")
                                              .DataTextField("DescripcionEmpresa")
                                              .DataValueField("CodEmpresa")
                                              .HtmlAttributes(new { style = "width:100%" })
                                              .Filter(FilterType.StartsWith)
                                              .IgnoreCase(true)
                                              .Placeholder("Seleccione un titular...")
                                              .DataSource(source =>
                                              {
                                                  source.Read(read =>
                                                  {
                                                      read.Action("GetApoderado", "Gestion");
                                                  });

                                              })
                                            )
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="form-group-sm">
                                        <div class="col-lg-6">
                                            <p> <strong>Firmante(s):</strong></p>
                                            @*<input class="form-control" id="firmantePoder" name="firmantePoder" type="text" placeholder="Firmante" required autocomplete="off">*@
                                            @(Html.Kendo().MultiSelect()
                                          .Name("firmantePoder")
                                          .DataTextField("DescAgente")
                                          .DataValueField("CodAgente")
                                          .Placeholder("Seleccionar firmantes...")
                                          //.MaxSelectedItems(3)
                                          .AutoBind(false)
                                          .DataSource(source =>
                                          {
                                              source.Read(read =>
                                              {
                                                  read.Action("GetAgente", "Gestion");
                                              });
                                              //.ServerFiltering(true);
                                          })
                                            )
                                        </div>
                                    </div>

                                    <div class="col-lg-6">

                                        <p><strong>Fecha de Creación:</strong></p>

                                        @(Html.Kendo().DateTimePicker()
                                            .Name("FechaGen")
                                            //.Value(DateTime.Now)
                                            .HtmlAttributes(new { style = "width:100%;" })
                                        )

                                    </div>


                                </div>
                                <br />
                                <div class="row" style="margin-top:15px;">
                                    <div class="form-group-sm">
                                        <div class="col-lg-6">
                                            <p> <strong>Poder Descripción:</strong></p>
                                            <textarea class="form-control" id="poderDescripcion" name="poderDescripcion" type="text" placeholder="Descripción Poder" required autocomplete="off"></textarea>
                                        </div>
                                    </div>
                                    <div class="form-group-sm">
                                        <div class="col-lg-6">
                                            <p> <strong>Observación:</strong></p>
                                            <textarea class="form-control" id="poderObservacion" name="poderObservacion" type="text" placeholder="Observación..." autocomplete="off"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <div class="col-lg-10">
                                                <p><strong>Adjuntar Escaneo de Poder:</strong></p>
                                                @(Html.Kendo().Upload()
                                                    .Name("scanPoder")
                                                )
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <a class="alert-link" data-toggle="modal" data-target="#poderScan"> Ver Escaneo de Poder</a>
                                        <section>
                                            <div class="modal fade" id="poderScan" role="dialog" aria-labelledby="myModalLabel">
                                                <div class="modal-dialog" role="document" style="width:40%">
                                                    <div class="modal-content">
                                                        <div class="modal-header">
                                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                                            <h4 class="modal-title" id="myModalLabel">Escaneo de Poder</h4>
                                                        </div>
                                                        <div class="modal-body">
                                                            <div class="row">
                                                                <div class="form-group-sm">
                                                                    <div class="col-lg-10">
                                                                        @(Html.Kendo().Grid<TelerikMvcApp4.Models.MntPoder>()
                    .Name("gridPoder")
                    .HtmlAttributes(new { style = "height:335px;" })
                    .Columns(columns =>
                    {
                        columns.Template(e => { }).ClientTemplate(" ").Width(120).Title("Imagen").HtmlAttributes(new { style = "height:10px" });
                        columns.Bound(e => e.Poder).Width(100).Title("Nº Poder").HtmlAttributes(new { style = "height:40px" });
                        columns.Bound(e => e.DescPoder).Width(100).Title("Desc. Poder").HtmlAttributes(new { style = "height:40px" });
                    })
                    .AutoBind(false)
                    .ClientRowTemplate(
                        "<tr id='a' data-uid='#: uid #'>" +
                            "<td class='photo'>" +
                            "<img src='data:image/png;base64,#: imagen64 #' alt='#: data.Poder #' style='width: 180px; height:100px;'/>" +
                            "</td>" +
                         "<td class='details'>" +
                             "<span class='title'>#: Poder #</span>" +
                         "</td>" +
                         "<td class='details'>" +
                             "<span class='title'>#: DescPoder #</span>" +
                         "</td>" +
                         "</tr>"
                    )

                    .DataSource(dataSource => dataSource
                        .Ajax()
                        .Batch(true)
                        .PageSize(10)
                        .ServerOperation(false)

                        .Model(model =>
                        {
                            model.Id(p => p.Poder);
                        })
                        .Read(read => read.Action("GrillaPoder", "Mantenimiento").Data("getParameterPoder"))
                    //.Events(events => events.RequestEnd("onRequestEnd"))

                    )
                    .Scrollable()
                                                                        )

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </section>
                                    </div>
                                </div>

                                <br />
                                <div class="form-group-sm">
                                    <button class="btn btn-primary" type="submit">Registrar</button>
                                    <button class="btn btn-primary" type="reset">Cancelar</button>
                                </div>
                                @*<div class="alert alert-success">
                                    <strong>Success!</strong> Indicates a successful or positive action.
                                </div>
                                <div class="alert alert-warning">
                                    <strong>Warning!</strong> Indicates a warning that might need attention.
                                </div>*@
                            }
                        </div>
                    </div>
                    <br />
                    <div class="panel panel-primary panel-Mantenimiento dvSombreadoMnt" id="Agente">
                        <div class="panel-heading panel-MantenimientoHead"><p style="color:white;font-size:14px;font-family:'Segoe UI Light';">Agente</p></div>
                        <div class="panel-body">
                            @using (Html.BeginForm("GetAgente", "Mantenimiento", FormMethod.Post, new { @id = "Frm-login", role = "form", @class = "form-horizontal" }))
                            {
                                <fieldset>
                                    <legend>Mantenimiento de Agentes</legend>
                                    <div class="row">
                                        <div class="col-lg-2">
                                            <p> <strong>Código:</strong></p>
                                            <input class="form-control" id="codigoAgente" name="codigoAgente" type="number" placeholder="Código" required autocomplete="off" onkeypress="return pulsarAgente(event)">
                                        </div>
                                    </div>
                                    <div class="row">

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <div class="col-lg-10">
                                                    <p><strong>Nombre:</strong></p>
                                                    <input class="form-control" id="nombreAgente" name="nombreAgente" type="text" placeholder="Nombre del agente" required autocomplete="off">
                                                </div>
                                            </div>
                                            @*<div class="form-group">
                                                <div class="col-lg-10">
                                                    <p><strong>Apellido:</strong></p>
                                                    <input class="form-control" id="apellidoAgente" name="apellidoAgente" type="text" placeholder="Apellido">
                                                </div>
                                            </div>*@
                                            <div class="form-group">
                                                <div class="col-lg-10">
                                                    <p><strong>Matricula:</strong></p>
                                                    <input class="form-control" id="matriculaAgente" name="matriculaAgente" type="text" placeholder="Matricula" autocomplete="off">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <button class="btn btn-primary" type="submit">Aceptar</button>
                                    <button class="btn btn-primary" type="reset">Cancelar</button>
                                    @*<button type="button" class="btn btn-default" data-toggle="modal" data-target="#editarAgente">
                                       <span class="glyphicon glyphicon-pencil"></span>
                                    </button>*@


                                </fieldset>
                            }

                        </div>
                    </div>
                    <br />
                    <div class="panel panel-primary panel-Mantenimiento dvSombreadoMnt" id="Periodico">
                        <div class="panel-heading panel-MantenimientoHead"><p style="color:white;font-size:14px;font-family:'Segoe UI Light';">Periódico</p></div>
                        <div class="panel-body">
                            @using (Html.BeginForm("GetPeriodico", "Mantenimiento", FormMethod.Post, new { @id = "Frm", role = "form", @class = "form-horizontal" }))
                            {
                                <fieldset>
                                    <legend>Mantenimiento de Periódico</legend>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <p> <strong>Código:</strong></p>
                                            <input class="form-control" id="codigoPeriodico" name="codigoPeriodico" type="number" placeholder="Código" required autocomplete="off" onkeypress="return pulsarPeriodico(event)">
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <div class="col-lg-10">
                                                    <p><strong>Periódico:</strong></p>
                                                    <input class="form-control" id="PeriodicoNombre" name="PeriodicoNombre" type="text" placeholder="Nombre del Periódico" required autocomplete="off">
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <br />
                                    <button class="btn btn-primary" type="submit">Aceptar</button>
                                    <button class="btn btn-primary" type="reset">Cancelar</button>
                                    @*<button type="button" class="btn btn-default" data-toggle="modal" data-target="#editarPeriodico">
                                        <span class="glyphicon glyphicon-pencil"></span>
                                    </button>*@

                                </fieldset>
                            }
                        </div>
                    </div>
                    <br />
                    <div class="panel panel-primary panel-Mantenimiento dvSombreadoMnt" id="marca">
                        <div class="panel-heading panel-MantenimientoHead"><p style="color:white;font-size:14px;font-family:'Segoe UI Light';">Marca</p></div>
                        <div class="panel-body">
                            @using (Html.BeginForm("GetMarca", "Mantenimiento", FormMethod.Post, new { @id = "Frm", role = "form", @class = "form-horizontal" }))
                            {
                                <fieldset>
                                    <legend>Mantenimiento de Marca</legend>
                                    <div class="row">
                                        <div class="col-lg-2" style="margin-bottom:10px">
                                            <p> <strong>Código:</strong></p>
                                            <input class="form-control" id="codigoMarca" name="codigoMarca" type="number" placeholder="Código" required autocomplete="off" onkeypress="return pulsarMarca(event)">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <div class="col-lg-10">
                                                    <p><strong>Denominación:</strong></p>
                                                    <input class="form-control" id="MarcaNombre" name="MarcaNombre" type="text" placeholder="Nombre de la Marca" autocomplete="off" style="text-transform: uppercase">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <div class="col-lg-10">
                                                    <p><strong>Nº Poder:</strong></p>
